# n8n example env (DO NOT COMMIT real secrets)
# Copy this file to n8n/.env and fill values locally.

# Basic auth for n8n (optional, recommended in prod)
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=
N8N_BASIC_AUTH_PASSWORD=

# n8n runtime
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
GENERIC_TIMEZONE=America/Bogota
N8N_ENCRYPTION_KEY=

# Integraci√≥n con Laravel Agent
# Use AGENTE_API_BASE=http://host.docker.internal:8000 when running inside Docker on Windows
AGENTE_API_BASE=
AGENTE_API_TOKEN=

# Optional callbacks
OUTLOOK_TIMEZONE=America/Bogota
GRAPH_USER_ID=me
RETURN_URL=http://localhost:8000/thanks
CANCEL_URL=http://localhost:8000/cancel
